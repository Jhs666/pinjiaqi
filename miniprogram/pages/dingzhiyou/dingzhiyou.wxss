.img {
  width: 100%;
  height: 100%;
  position: relative;
}

.img .img_bg {
  width: 100%;
  height:600px;
  display: block;
  margin: 0 auto;
}

.img .bg {
  width: 94%;
  margin: 0 auto;
  border-radius: 8px;
  box-shadow: 0 0 16px #ddd;
  background: #fff;
  position: absolute;
  top: 200rpx;
  left: 3%;
  z-index: 999;
}

.swiper-tab {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 4% 0;
}

.swiper-tab-item {
  width: 24%;
  float: left;
  color: #bbb;
  text-align: center;
  margin: 0 13%;
  padding-bottom: 5%;
  font-size: 36rpx;
}

.active {
  color: #333;
  border-bottom: 6rpx solid #06b3ed;
}

swiper {
  text-align: center;
  position: relative;
  z-index: 999;
  width: 84%;
  height: 530rpx;
  margin: 0 auto;
}

swiper .btn {
  width: 100%;
  margin: 4% auto;
  background: #06b3ed;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
  line-height: 76rpx;
  border-radius: 30px;
}

swiper swiper-item .list {
  width: 100%;
  overflow: hidden;
}

swiper swiper-item .list .first {
  position: relative;
}

swiper swiper-item .list .li {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  border-bottom: 1px solid #efefef;
}

swiper swiper-item .list .li .start {
  font-size: 32rpx;
  color: #666;
  float: left;
  line-height: 100rpx;
  padding: 0 3%;
}

swiper swiper-item .list .li .end {
  font-size: 32rpx;
  color: #666;
  float: right;
  line-height: 100rpx;
  padding: 0 3%;
}

swiper swiper-item .list .li image {
  width: 64rpx;
  height: 30rpx;
  margin: 35rpx auto;
  display: inline-block;
  position: absolute;
  left: 0;
  right: 0;
}

swiper swiper-item .list .li input {
  width: 100%;
  height: 100rpx;
  text-align: left;
  font-size: 32rpx;
  color: #bbb;
  line-height: 100rpx;
}
