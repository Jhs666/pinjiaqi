.history{
  width: 170rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 3%;
  text-align: center;
  position: absolute;
  top:9%;
  right: 3%;
  background: rgba(0,0,0,0.6);
  border-radius: 30px;
}
.history image{
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin: 10rpx 0;
}
.text{
  color: #fff;
  font-size: 28rpx;
  float: left;
  margin-left: 5%;
}
.img {
  width: 100%;
  height: 100%;
  position: relative;
}

.img .img_bg {
  width: 100%;
  display: block;
  margin: 0 auto;
}

.img .bg {
  width: 94%;
  margin: 0 auto;
  border-radius: 8px;
  box-shadow: 0 0 16px #ddd;
  background: #fff;
  position: absolute;
  top: 200rpx;
  left: 3%;
  z-index: 999;
}

.swiper-tab {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 4% 0;
}

.swiper-tab-item {
  width: 24%;
  float: left;
  color: #bbb;
  text-align: center;
  margin: 0 13%;
  padding-bottom: 5%;
  font-size: 36rpx;
}

.active {
  color: #333;
  border-bottom: 6rpx solid #06b3ed;
}

swiper {
  text-align: center;
  position: relative;
  z-index: 999;
  width: 84%;
  height: 750rpx;
  margin: 0 auto;
}

.btn {
  width: 100%;
  margin: 4% auto;
  background: #06b3ed;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
  line-height: 76rpx;
  border-radius: 30px;
}

.list {
  width: 100%;
  overflow: hidden;
}

.list .first {
  position: relative;
}

.list .li {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  border-bottom: 1px solid #efefef;
}
.dl{
  width: 100%;
  height: 120rpx;
  overflow: hidden;
  border-bottom: 1px solid #efefef;
}
.dl textarea{
  width: 100%;
  height: 100%;
  color: #bbb;
  font-size: 28rpx;
  text-align: left;
  padding-top: 2%;
}

.li .start {
  width: 30%;
  text-align: left;
  font-size: 32rpx;
  color: #666;
  float: left;
  line-height: 100rpx;
}

.li .end {
  width: 30%;
  text-align: right;
  font-size: 32rpx;
  color: #666;
  float: right;
  line-height: 100rpx;
}

.li image {
  width: 64rpx;
  height: 30rpx;
  margin: 35rpx auto;
  display: inline-block;
  position: absolute;
  left: 0;
  right: 0;
}

.li input {
  width: 100%;
  height: 100rpx;
  text-align: left;
  font-size: 32rpx;
  color: #bbb;
  line-height: 100rpx;
}
.li picker {
  width: 100%;
  height: 100rpx;
  text-align: left;
  font-size: 32rpx;
  color: #777;
  line-height: 100rpx;
}

.transition {
  transition: all 0.5s ease-out;
}

.left {
  transform: translate3d(280%, 0, 0);
}

.right {
  transform: translate3d(-280%, 0, 0);
}
