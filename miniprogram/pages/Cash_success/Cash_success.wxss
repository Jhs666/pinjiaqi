.center{
  padding-top: 3%;
}
.cash{
  width: 94%;
  height: 445px;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}
.cash .img{
  width: 100%;
  height: 100%;
  display: block;
  margin:0 auto;
}
.logo{
  width: 160rpx;
  height: 160rpx;
  margin: 0 auto;
  overflow: hidden;
}
.logo image{
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
}
.text{
  color: #fff;
  font-size: 28rpx;
  width: 440rpx;
  margin: 0 auto;
  text-align: justify;
  margin-top: 7%;
  line-height: 42rpx;
}
.confirm{
  width: 60%;
  height: 76rpx;
  color: #fff;
  text-align: center;
  line-height: 76rpx;
  font-size: 32rpx;
  border:2rpx solid #fff;
  border-radius: 40rpx;
  position: absolute;
  bottom: 6%;
  left: 20%;
}
.commit{
  width: 100%;
  height: 75%;
  position: absolute;
  top: 0;
  padding: 14% 0 10%;
  z-index: 99;
}