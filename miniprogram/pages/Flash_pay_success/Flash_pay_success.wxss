.youhui{
    width: 94%;
    height: 760rpx;
    margin: 3% auto 0;
    overflow: hidden;
    position: relative;
}
.bg{
    width: 100%;
    height: 760rpx;
    display: block;
    margin: 0 auto;
}
.youhui .content{
    width: 92%;
    overflow: hidden;
    position: absolute;
    top: 5%;
    left: 4%;
}
.bottom_line{
    width: 90%;
    height: 4rpx;
    border-top: 4rpx dashed #eee;
    position: absolute;
    left: 5%;
    top: 39%;
}
.youhui .content .title{
    font-size: 32rpx;
    line-height: 44rpx;
    color: #333;
    text-align: justify;
}
.youhui .content .success{
    font-size: 24rpx;
    line-height: 40rpx;
    color: #999;
}
.youhui .content .money text{
    color: #999;
    font-size: 24rpx;
    line-height: 52rpx;
    float: left;
}
.youhui .content .money rich-text{
    color: #ff0000;
    font-size: 36rpx;
    line-height: 50rpx;
    float: left;
}
.youhui .shiyong{
    width: 92%;
    height: auto;
    overflow: hidden;
    position: absolute;
    left: 4%;
    bottom: 3%;
}
.success_img{
    width: 180rpx;
    height: 180rpx;
    margin: 0 auto;
}
.success_img image{
    width: 100%;
    height: 100%;
    display: block;
    margin: 0 auto;
}
.res{
    font-size: 36rpx;
    text-align: center;
    display: block;
    color: #06B3ED;
    line-height: 50rpx;
    margin: 5% 0;
}
.back{
    width:104rpx;
    height: 52rpx;
    line-height: 52rpx;
    color: #fff;
    background: #06B3ED;
    text-align: center;
    border-radius: 8rpx;
    margin: 0 auto;
    font-size: 28rpx;
    margin-bottom: 5%;
}