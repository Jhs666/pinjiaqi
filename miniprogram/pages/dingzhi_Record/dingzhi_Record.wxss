.center{
  padding-top: 3%;
}
.type{
  width: 200rpx;
  height: 60rpx;
  float: left;
  background: linear-gradient(to right, #7bdbfb, #06b3ed);
  margin-left: 3%;
}
.type image{
  width: 32rpx;
  height: 32rpx;
  float: left;
  margin: 15rpx 0 15rpx 8%;
}
.text{
  color: #fff;
  font-size: 32rpx;
  line-height: 60rpx;
  float: left;
  margin-left: 3%;
}
.li{
  width: 88%;
  height: auto;
  margin: 0 auto;
  overflow: hidden;
  background: #fff;
  box-shadow: 0 0 20rpx #ddd;
  border-radius: 12rpx;
  margin-bottom: 3%;
  padding: 3%;
}
.topbar{
  width: 100%;
  height: 80rpx;
  overflow: hidden;
}
.topbar image {
  width: 76rpx;
  height: 28rpx;
  margin: 28rpx auto;
  display: inline-block;
  position: absolute;
  left: 0;
  right: 0;
}
.start {
  width: 30%;
  text-align: center;
  font-size: 32rpx;
  color: #666;
  float: left;
  line-height: 80rpx;
}
.end {
  width: 30%;
  text-align: center;
  font-size: 32rpx;
  color: #666;
  float: right;
  line-height: 80rpx;
}
.dl{
  width: 100%;
  height: auto;
  font-size: 28rpx;
  color:#999;
  line-height:60rpx;
}
.kong {
  width: 100%;
  height: auto;
  margin: 0 auto;
  text-align: center;
}
.kong image {
  width: 280rpx;
  height: 288rpx;
  display: block;
  margin: 35% auto 0;
}
.kong text {
  color: #999;
  font-size: 28rpx;
  line-height: 40rpx;
  display: block;
  margin-top: 8%;
}