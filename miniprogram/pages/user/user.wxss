.top {
  width: 100%;
  height: 564rpx;
  overflow: hidden;
}
.back {
  width: 94%;
  height: 300rpx;
  background: linear-gradient(to bottom, #06b3ed 0%, #0152de 100%);
  padding: 0 3%;
  position: relative;
}
.shezhi {
  width: 42rpx;
  height: 40rpx;
  overflow: hidden;
  position: absolute;
  top: 3%;
  left: 3%;
}
.shezhi image {
  width: 100%;
  height: 40rpx;
}
.xiaoxi {
  width: 44rpx;
  height: 40rpx;
  overflow: hidden;
  position: absolute;
  top: 3%;
  right: 3%;
}
.xiaoxi image {
  width: 100%;
  height: 40rpx;
}
.red_dian{
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  position: absolute;
  top: 0;
  right: 0;
  background: #ff0000;
}
.user {
  width: 94%;
  height: auto;
  margin: 0 auto;
  background: #fff;
  border-radius: 8px;
  box-shadow: 0 0 16px #ddd;
  position: absolute;
  top: 30%;
  z-index: 999;
}
.user .touxiang {
  width: 96%;
  overflow: hidden;
  padding: 4% 2%;
  position: relative;
}
.user .touxiang .img {
  width: 18%;
  height: 120rpx;
  overflow: hidden;
  border-radius: 50%;
  float: left;
  margin:34rpx 0; 
}
.user .touxiang .img image {
  width: 100%;
  height: 120rpx;
  display: block;
  margin: 0 auto;
}
.user .touxiang .desc {
  width: 78%;
  float: left;
  margin-left: 4%;
  padding: 3% 0;
}
.user .touxiang .desc .fr {
  width: 100%;
  overflow: hidden;
}
.user .touxiang .desc .name {
  font-size: 32rpx;
  color: #000;
  float: left;
  line-height: 50rpx;
}
.user .touxiang .desc .tag {
  color: #07c1f0;
  font-size: 20rpx;
  border: 1px solid #07c1f0;
  float: left;
  padding: 4rpx;
  margin: 1% 0 0 4%;
}
.user .touxiang .desc rich-text {
  font-size: 24rpx;
  color: #999;
  line-height: 50rpx;
}
.user .touxiang .desc .house {
  width: 206rpx;
  height: 56rpx;
  float: right;
  background: #ffc802;
  position: absolute;
  right: 0;
  bottom: 12%;
  border-radius: 30px 0px 0px 30px;
  padding-left: 2%;
}
.user .touxiang .desc .house image {
  width: 50rpx;
  height: 50rpx;
  float: left;
  margin: 4rpx 0;
}
.user .touxiang .desc .house text {
  font-size: 22rpx;
  color: #fff;
  float: left;
  line-height: 56rpx;
  margin-left: 2%;
}
.user .ul {
  width: 96%;
  padding: 2% 0;
  margin: 0 auto;
  overflow: hidden;
  border-top: 2rpx solid #efefef;
  border-bottom: 2rpx solid #efefef;
}
.user .ul .li {
  float: left;
  width: 50%;
  text-align: center;
}
.user .ul .li text {
  font-size: 32rpx;
  color: #000;
  line-height: 44rpx;
  font-weight: bolder;
}
.user .ul .li rich-text {
  font-size: 28rpx;
  color: #999;
  line-height: 50rpx;
}
/* 我的订单 */
.goods {
  width: 94%;
  background: #fff;
  overflow: hidden;
  padding: 0 3%;
}
.goods .ul {
  width: 100%;
  overflow: hidden;
  margin: 0 auto;
}
.order_title {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  overflow: hidden;
  border-bottom: 2rpx solid #efefef;
}
.order_title .titles {
  float: left;
  color: #000;
  font-size: 32rpx;
  font-weight: bolder;
}
.all_order {
  width: 25%;
  float: right;
  overflow: hidden;
}
.all_order text {
  font-size: 24rpx;
  color: #666;
  float: left;
}
.all_order image {
  width: 24rpx;
  height: 24rpx;
  float: right;
  margin: 23rpx 0;
}
.goods .ul {
  margin: 3% 0;
}
.goods .ul .li {
  width: 20%;
  float: left;
  text-align: center;
}
.img_icon{
  width: 46rpx;
  height: 46rpx;
  overflow: hidden;
  margin: 0 auto;
}
.goods .ul .li image {
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
}
.goods .title {
  font-size: 32rpx;
  color: #000;
  line-height: 44rpx;
  font-weight: bolder;
}

.goods .ul .li .small {
  font-size: 24rpx;
  color: #666;
  line-height: 34rpx;
  letter-spacing: 2rpx;
}
/* 常用工具 */
.tool {
  width: 94%;
  background: #fff;
  overflow: hidden;
  padding: 3% 3%;
  margin: 3% 0;
}
.tool .ul {
  width: 100%;
  overflow: hidden;
  margin: 4% 0 0;
}
.tool .ul .li {
  width: 20%;
  float: left;
  text-align: center;
}
.tool .ul .li image {
  width: 46rpx;
  height: 44rpx;
  display: block;
  margin: 0 auto;
}
.tool .title {
  font-size: 32rpx;
  color: #000;
  line-height: 44rpx;
  font-weight: bolder;
}
.tool .ul .li .small {
  font-size: 24rpx;
  color: #666;
  line-height: 34rpx;
  letter-spacing: 2rpx;
}
/* 猜你喜欢 */
.like {
  width: 94%;
  background: #fff;
  overflow: hidden;
  padding: 3% 3%;
  margin: 3% 0;
}
.like .ul {
  width: 100%;
  overflow: hidden;
  margin: 4% 0 2%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.like .title {
  font-size: 32rpx;
  color: #000;
  line-height: 44rpx;
  font-weight: bolder;
}
.like .ul .li {
  width: 220rpx;
  float: left;
  border: 1px solid #efefef;
}
.like .ul .li .img {
  width: 100%;
  height: 170rpx;
  overflow: hidden;
  position: relative;
}
.like .ul .li .img image {
  width: 100%;
  height: 170rpx;
}
.like .ul .li .img .zuijin {
  color: #000;
  font-size: 20rpx;
  background: #43eff9;
  text-align: center;
  line-height: 28rpx;
  position: absolute;
  top: 0;
  left: 0;
  padding: 0 2%;
}
.like .ul .li .img .tag {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 30rpx;
}
.like .ul .li .img .tag text {
  background: rgba(0, 0, 0, 0.4);
  color: #fff;
  text-align: center;
  font-size: 20rpx;
  line-height: 28rpx;
  float: left;
  margin-right: 5%;
  padding: 0 2%;
}
.like .ul .li .text {
  width: 100%;
  text-align: center;
  margin: 5% 0;
}
.like .ul .li .text .zhu {
  color: #000;
  font-size: 20rpx;
  line-height: 36rpx;
}
.like .ul .li .text .fu {
  color: #9b9b9b;
  font-size: 20rpx;
  line-height: 28rpx;
}
.shouquan {
  color: #333;
  font-size: 28rpx;
  line-height: 80rpx;
  border-radius: 8rpx;
  background: #fff;
  border: none;
}
.btn{
  background: none;
  text-align: center;
  padding: 0;
  margin: 0;
  line-height: 56rpx;
}
.btn::after{
  border: none
}
.balance{
  width: 94%;
  margin: 0 auto;
  color: #999;
  font-size: 28rpx;
  line-height: 70rpx;
}
.total{
  float: right;
}
.balance image {
  width: 24rpx;
  height: 24rpx;
  float: right;
  margin: 23rpx 0 23rpx 10rpx;
}