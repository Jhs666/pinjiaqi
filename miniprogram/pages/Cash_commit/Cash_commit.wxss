.center{
  padding-top: 3%;
}
.cash{
  width: 94%;
  height: 445px;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}
.cash .img{
  width: 100%;
  height: 100%;
  display: block;
  margin:0 auto;
}
.title{
  color: #fff;
  font-size:64rpx;
  text-align: left; 
}
.price{
  color: #fff;
  font-size: 40rpx;
  text-align: left;
  margin-top: 5%;
  border-bottom: 4rpx solid rgba(255,255,255,0.8);
  overflow: hidden;
  height: 100rpx;
  line-height: 100rpx;
}
input{
  width: 80%;
  height: 100%;
  line-height: 100rpx;
  font-size: 40rpx;
  color: #fff;
  text-align: left;
  display: inline-block;
  overflow: visible;
}
.phcolor{
  color: rgba(255,255,255,0.8);
}
.confirm{
  width: 60%;
  height: 76rpx;
  color: #fff;
  text-align: center;
  line-height: 76rpx;
  font-size: 32rpx;
  border:2rpx solid #fff;
  border-radius: 40rpx;
  position: absolute;
  bottom: 6%;
  left: 20%;
}
.commit{
  width: 92%;
  height: 75%;
  position: absolute;
  top: 0;
  padding: 10% 4% 12%;
  z-index: 99;
}
.btn{
  color:#fff;
  font-size: 28rpx;
  float: right;
  margin-top: 10%;
}
.total{
  width: 100%;
  overflow: hidden;
  margin-top: 3%;
}
.money{
  float: left;
  color: #fff;
  font-size: 28rpx;
}
.time{
  float: right;
  color: #fff;
  font-size: 28rpx;
}