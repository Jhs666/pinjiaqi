page{
  background: #fa533d;
}
.center{
  position: relative;
}
.topbar{
  width: 100%;
  height: auto;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
}
.topbar image{
  width: 100%;
  display: block;
  margin: 0 auto;
}
.arrow {
  width: 24rpx;
  height: 24rpx;
  overflow: hidden;
  position: fixed;
  left: 3%;
  z-index: 9999;
  border-left: 4rpx solid #fff;
  border-top: 4rpx solid #fff;
  transform: rotate(-45deg);
}

.arrow_two {
  width: 24rpx;
  height: 24rpx;
  overflow: hidden;
  position: fixed;
  left: 3%;
  z-index: 99999;
  transform: rotate(-45deg);
}
.bar {
  width: 100%;
  height: auto;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99999;
}

.top_title {
  font-size: 36rpx;
  text-align: center;
  color: #fff;
  line-height: 42px;
}
.list_show {
  width: 94%;
  margin: 0 auto;
  position: absolute;
  top: 270px;
  left: 3%;
  z-index: 99;
}

.li {
  width: 100%;
  height: 230rpx;
  overflow: hidden;
  margin: 0 auto;
  margin-bottom: 3%;
  background: #fff;
  border-radius: 8px;
  box-shadow: 0 0 10px #f33d05;
  position: relative;
}

.img {
  width: 40%;
  height: 100%;
  float: left;
  position: relative;
}

.img image {
  width: 100%;
  height: 230rpx;
  display: block;
  margin: 0 auto;
}

.title {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background: rgba(0, 0, 0, 0.4);
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  font-size: 20rpx;
}

.con {
  float: left;
  width: 54%;
  height: 200rpx;
  position: relative;
  padding: 2% 3%;
}

.desc {
  color: #000;
  font-size: 28rpx;
  line-height: 46rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-align: left;
}
.jindu{
  width: 90%;
  overflow: hidden;
  position: absolute;
  bottom: 30%;
  left: 5%;
}
.aa {
  width: 90%;
  overflow: hidden;
  position: absolute;
  bottom: 5%;
  left: 5%;
}

.Original {
  color: #ff001f;
  font-size: 30rpx;
  float: left;
  display: block;
  line-height: 46rpx;
}

.btn {
  font-size: 23rpx;
  color: #fff;
  border: 2rpx solid #ee4a5d;
  border-radius: 60rpx;
  width: auto;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  float: right;
  padding: 0 5%;
  background: linear-gradient(to right, #ff001f, #ee4a5d);
}

.quota {
  margin-left: 20rpx;
}

.bb {
  width: 60%;
  overflow: hidden;
  margin: 5% 0 3%;
  border-radius: 50rpx;
  float: left;
}

.success {
  color: #f00;
  text-align: left;
  float: right;
  margin: 2% 0 3%;
  font-size: 24rpx;
}

.success text {
  color: #f00;
}
.kong{
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  top: 85px;
  left: 0;
  z-index: 99;
}