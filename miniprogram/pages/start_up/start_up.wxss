.skip{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  color: #fff;
  position: fixed;
  right: 5%;
  bottom: 10%;
  z-index: 99999;
  background: rgba(0,0,0,0.4);
  border-radius: 30rpx;
  padding: 0 3%;
  text-align: center;
}
.go_home{
  margin-left: 5%;
}
.start_up{
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: relative;
}
.ban {
  width: 100%;
}
.slide-image {
  width: 100%;
}
.dots{ 
  position: absolute; 
  left: 0; 
  right: 0; 
  bottom: 12%; 
  display: flex; 
  justify-content: center; 
} 
.dots .dot{ 
  margin: 0 8rpx; 
  width: 14rpx; 
  height: 14rpx; 
  background: #fff; 
  border-radius: 8rpx; 
  transition: all .6s; 
} 
.dots .dot.active{ 
  width: 30rpx; 
  background:#06B3ED; 
}

.animate {
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: rgba(0, 0, 0, 0.6);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 999999;
}

.animate_windows {
  position: absolute;
  z-index: 9999999;
  width: 750rpx;
  height: 174rpx;
  top: 36%;
  left:50%;
  margin-left: -32%;
  word-wrap: break-word;
  animation-name: popup;
  animation-duration: 0.2s;
  animation-timing-function: linear;
  animation-delay: 0;
  animation-iteration-count: 1;
  animation-direction: normal;
}
.eject{
  width: 60%;
  height: auto;
  color: #333;
  border-radius: 4px;
  background: #fff;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
  padding: 3%;
}
.login_title{
  font-size: 32rpx;
  border-bottom: 2rpx solid #efefef;
  padding: 0 0 5%;
}
.login_con{
  border-bottom: 2rpx solid #efefef;
  padding: 3% 0;
  line-height: 46rpx;
}
.eject button{
  background: #06B3ED;
  color: #fff;
  line-height: 70rpx;
  padding: 0;
  font-size: 32rpx;
  border-radius:4px;
  margin-top: 5%;
}

.btn {
  width: 272rpx;
  height: 90rpx;
  position: absolute;
  z-index: 9999;
  background: rgba(0, 0, 0, 0);
  top: 83%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 60rpx;
}

@keyframes popup {
  from {
    opacity: 0;
    transform: scale(0.3, 0.3);
  }
  to {
    opacity: 1;
    transform: scale(1, 1);
  }
}