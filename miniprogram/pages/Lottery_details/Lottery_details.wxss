page{
  background: #81a2ff;
}
.search {
  width: 94%;
  margin: 0 auto;
  padding: 3% 3%;
  overflow: hidden;
  border-bottom: 1px solid #efefef;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}

.search .left {
  width: 84%;
  height: 60rpx;
  background: #f0f0f0;
  float: left;
  padding: 0 0 0 2%;
  border-radius: 30rpx;
  overflow: hidden;
}

.search .left image {
  width:7%;
  height: 40rpx;
  float: left;
  margin: 10rpx 5rpx 10rpx 0;
}

.search .left input {
  width: 72%;
  height: 60rpx;
  color: #9b9b9b;
  font-size: 24rpx;
  line-height: 34rpx;
  float: left;
}

.search text {
  font-size: 28rpx;
  color: #666;
  line-height: 60rpx;
  float: right;
}
.search_btn{
  width: 15%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #9b9b9b;
  text-align: center;
  float: right;
  border-left: 2rpx solid #ddd;
}
.img_bg{
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
  margin-top: 103rpx;
}
.box{
  width: 96%;
  margin: 0 auto;
  position: absolute;
  top: 430rpx;
  left: 2%;
}
.wallet_li{
  width: 92%;
  height: auto;
  overflow: hidden;
  margin: 0 auto;
  background: #fff;
  box-shadow: 0 0 18rpx #5fa0ff;
  border-radius: 10rpx;
  padding:5% 3% 8%;
}
.img_title{
  width: 100%;
  overflow: hidden;
}
.left_img{
  width: 18%;
  height: 120rpx;
  border-radius: 16rpx;
  float: left;
  overflow: hidden;
}
.left_img image{
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
}
.title{
  width: 78%;
  float: left;
  margin-left: 3%;
}
.tit{
  font-size: 32rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
}
.price{
  color: #ff0000;
  font-size: 28rpx;
}
.frame{
  width: 80%;
  height: 120rpx;
  float: right;
  overflow: hidden;
  margin-top: 3%;
  position: relative;
}
.tag_bg{
  width: 100%;
  height: 100%;
  display: block;
  margin:0 auto;
}
.left_show{
  position: absolute;
  top: 22%;
  left:6%;
  z-index: 99;
  width: 66%;
  overflow: hidden;
  border-right: 2rpx dashed #06B3ED;
}
.date{
  font-size: 20rpx;
  color: #666;
  float: left;
}
.order_number{
  color: #666;
  font-size: 20rpx;
  float: left;
  display: block;
  margin-top: 4%;
}
.right_show{
  position: absolute;
  top:38%;
  right: 0;
  z-index: 99;
  width: 28%;
  overflow: hidden;
}
.status{
  color: #06B3ED;
  font-size: 20rpx;
  display: block;
  width: 100%;
  text-align: center;
}
.look_btn{
  color: #fff;
  background: #06B3ED;
  border-radius: 60rpx;
  font-size: 20rpx;
  width: 76rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  margin: 5% auto 0;
  display: block;
}
.Winning_num{
  width: 98%;
  height: auto;
  overflow: hidden;
  margin: 0 auto;
  background: #fff;
  box-shadow: 0 0 18rpx #5fa0ff;
  border-radius: 10rpx;
  margin-top: 3%;
}
.num_title{
  text-align: center;
  font-size: 42rpx;
  color: #0077ff;
  background: #b3e2ff;
  line-height: 82rpx;
}
.num_con{
  height: 200rpx;
}
.result_num{
  font-size: 32rpx;
  color: #ff0000;
  text-align: center;
  line-height: 200rpx;
}
.all_result{
  width: 98%;
  height: 370rpx;
  overflow: hidden;
  margin: 0 auto;
  background: #fff;
  box-shadow: 0 0 18rpx #5fa0ff;
  border-radius: 10rpx;
  margin-top: 3%;
  position: relative;
  margin-bottom: 10%;
}
.line{
  width: 90%;
  height: 78%;
  margin: 0 auto;
  border:2rpx dashed #5fa0ff;
  margin-top: 6%;
  overflow: hidden;
}
.all_order{
  position: relative;
}
.lianjie_img{
  width: 40rpx;
  height:116rpx;
  position: absolute;
  bottom:-20%;
  left: 6%;
  z-index: 99;
}
.lianjie_img2{
  width: 40rpx;
  height:116rpx;
  position: absolute;
  bottom:-20%;
  right: 6%;
  z-index: 99;
}
.zhongjian{
  position: relative;
}
.roll{
  width: 94%;padding: 3%;
}
.num_list{
  font-size: 28rpx;
  color: #666;
  text-align: center;
}
swiper-item{
  width: 100%;
  height: 54rpx !important;
  line-height:54rpx;
}