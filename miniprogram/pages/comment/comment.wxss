.imgss {
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
  float: left;
}
.center {
  padding-bottom: 20%;
}
.top {
  background: #fff;
  padding: 0 3%;
  width: 94%;
  margin: 3% auto 0;
  overflow: hidden;
}
.topbar {
  width: 100%;
  overflow: hidden;
  padding: 3% 0;
  border-bottom: 2rpx solid #efefef;
}
.img {
  width: 12.5%;
  height: 88rpx;
  overflow: hidden;
  float: left;
}
.img image {
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
}
.Route {
  width: 82.5%;
  float: left;
  margin-left: 5%;
}
.Route text {
  color: #333;
  font-size: 32rpx;
  line-height: 84rpx;
  float: left;
  margin-right: 3%;
  font-weight: bolder;
  display: block;
}
.star {
  float: left;
}
.star  image {
  height: 30rpx;
  width: 30rpx;
  display: block;
  float: left;
  margin: 30rpx 6rpx;
}
.part {
  float: left;
  font-size: 32rpx;
  color: #333;
  line-height: 84rpx;
  margin-left: 20rpx;
  font-weight: bolder;
}
.part1{
  float: left;
  font-size: 32rpx;
  color: #fff;
  line-height: 60rpx;
  margin-left: 20rpx;
  font-weight: bolder;
}
textarea {
  color: #bbb;
  font-size: 24rpx;
  padding: 2% 0;
  width: 100%;
  height: 150rpx;
  overflow: auto;
}
.photo {
  width: 152rpx;
  height: 152rpx;
  background: #fff;
  box-shadow: 0 0 16rpx #ddd;
  border-radius: 12rpx;
  float: left;
  text-align: center;
  margin: 0 0 3% 3%;
}
.photo image {
  width: 64rpx;
  height: 48rpx;
  display: block;
  margin: 20% auto 0;
}
.photo text {
  font-size: 24rpx;
  color: #999;
  line-height: 34rpx;
}
/* 匿名 */
.niming {
  width: 100%;
  overflow: hidden;
  border-top: 2rpx solid #efefef;
  padding: 3% 0;
}
.niming image {
  width: 32rpx;
  height: 32rpx;
  float: left;
}
.niming text {
  float: left;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #999;
  margin-left: 20rpx;
}
.title {
  font-size: 32rpx;
  line-height: 44rpx;
  color: #333;
  font-weight: bolder;
  display: block;
  margin-bottom: 2%;
}
.li {
  width: 100%;
  overflow: hidden;
}
.li text {
  font-size: 28rpx;
  color: #bbb;
  line-height: 92rpx;
  float: left;
  margin-right: 30rpx;
  width: 18%;
  text-align: right;
}
.dl text {
  line-height: 60rpx;
}
.dl image {
  margin: 15rpx 6rpx;
}
.btn {
  width: 100%;
  height: 100rpx;
  background: #06b3ed;
  line-height: 100rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  text-align: center;
}
.btn text {
  color: #fff;
  font-size: 36rpx;
  display: block;
}
.imgs {
  width: 152rpx;
  height: 152rpx;
  display: inline-block;
  margin: 0 0 3% 3%;
}
.imgss {
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
  float: left;
}