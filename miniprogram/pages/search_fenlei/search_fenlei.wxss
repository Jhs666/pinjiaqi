.search {
  width: 94%;
  margin: 0 auto;
  padding: 3% 3%;
  overflow: hidden;
  background: #fff;
  border-bottom: 2rpx solid #efefef;
}

.search .left {
  width: 84%;
  height: 60rpx;
  background: #f0f0f0;
  float: left;
  padding: 0 2%;
  border-radius: 30rpx;
}

.search .left image {
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin: 10rpx 5rpx 10rpx 0;
}

.search .left input {
  height: 60rpx;
  color: #9b9b9b;
  font-size: 24rpx;
  line-height: 34rpx;
}

.search text {
  font-size: 32rpx;
  color: #000;
  line-height: 60rpx;
  float: right;
}
.overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100%;
}

.content {
  display: flex;
  width: 100%;
  position: absolute;
  bottom: 0rpx;
  left: 0;
  top: 9%;
  overflow: hidden;
}

.menu-container {
  width: 190rpx;
  flex: 0 0 190rpx;
  background: #f6f6f6;
}

.detail-container {
  flex: 1;
  height: 100%;
}

.scroll-view {
  width: 100%;
  height: 100%;
}

.scroll-view-ul {
  width: 100%;
  height: 100%;
}

.scroll-view-item {
  width: 100%;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #333;
  border-left: 8rpx solid transparent;
  display: flex;
  align-items: left;
  justify-content: left;
  padding-left: 30rpx;
  position: relative;
}

.active {
  background: #fff;
  color: #333;
  border-left: 8rpx solid #06b3ed;
}

.market-scroll-list {
  width: 94%;
  margin: 0 auto;
  height: 100%;
  background: #fff;
  padding: 0 3%;
}

.list_title {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}

.list_title image {
  width: 42rpx;
  height: 34rpx;
  float: left;
  margin: 33rpx 0;
}

.title {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #666;
  margin-left: 20rpx;
}

.tag {
  width: 100%;
  overflow: hidden;
}

.taglist {
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
  background: #f5f5f9;
  float: left;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
}

.list_title2 {
  width: 100%;
  overflow: hidden;
}

.list_title2 image {
  width: 34rpx;
  height: 34rpx;
  float: left;
  margin: 15rpx 0;
}

.title2 {
  font-size: 28rpx;
  color: #666;
  margin-left: 20rpx;
}

.list_show {
  width: 100%;
  overflow: hidden;
}

.big {
  width: 100%;
  overflow: hidden;
  border: 2rpx solid #efefef;
  padding-bottom: 10rpx;
}

.big image {
  width: 100%;
  height: 180rpx;
  display: block;
  margin: 0 auto;
}

.biaoti {
  width: 100%;
  overflow: hidden;
  text-align: center;
}

.place {
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
}

.num {
  font-size: 24rpx;
  color: #999;
  line-height: 34rpx;
  margin-left: 20rpx;
}

.small {
  width: 100%;
  margin-top: 8%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.li {
  width: 240rpx;
  text-align: center;
  margin: 0 0 8% 0;
  border: 2rpx solid #efefef;
  padding-bottom: 10rpx;
}

.img {
  width: 100%;
  height: 180rpx;
  overflow: hidden;
}

.img image {
  width: 100%;
  height: 100%;
  display: block;
  margin: 0 auto;
}

.li .num {
  margin-left: 0;
}
/* 隐藏滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}